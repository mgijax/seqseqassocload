#format: sh
#
#  vegatranscript_seqseqassocload.config
#
#  This file sets up environment variables that are needed for 
#  the VEGA transcript to gene model association load
#
###########################################################################

###########################################################################
#
#  LOAD SPECIFIC SETTINGS
#
###########################################################################

#  Path name of the archive, logs, reports and output directories
FILEDIR=${DATALOADSOUTPUT}/vega/vega_transcript_seqseqassocload
ARCHIVEDIR=${FILEDIR}/archive
LOGDIR=${FILEDIR}/logs
RPTDIR=${FILEDIR}/reports
OUTPUTDIR=${FILEDIR}/output

# directory where input files are located
INPUTDIR=${DATALOADSOUTPUT}/vega/vega_transcriptseqload/output
# full path to the input file
INFILE_NAME=${INPUTDIR}/vega_transcript_seqseqassoc.txt

export FILEDIR ARCHIVEDIR LOGDIR RPTDIR OUTPUTDIR INPUTDIR INFILE_NAME

#  Complete path name of the log files
LOG_PROC=${LOGDIR}/vega_transcript_seqseqassocload.proc.log
LOG_DIAG=${LOGDIR}/vega_transcript_seqseqassocload.diag.log
LOG_CUR=${LOGDIR}/vega_transcript_seqseqassocload.cur.log
LOG_VAL=${LOGDIR}/vega_transcript_seqseqassocload.val.log

export LOG_PROC LOG_DIAG LOG_CUR LOG_VAL 

#  The name of the job stream for the load, MGI_User.login
JOBSTREAM=vegatrans_seqseqassocload
USERKEY=1499

export JOBSTREAM USERKEY

# Sequence1 Logical DB Key
SEQ1_LOGICAL_DBKEY=131 # VEGA transcript

# Sequence2 Logical DB Key
SEQ2_LOGICAL_DBKEY=85 # VEGA Gene Model

export SEQ1_LOGICAL_DBKEY SEQ2_LOGICAL_DBKEY

###########################################################################
#
#  MISCELLANEOUS SETTINGS
#
###########################################################################

#  The name of the load for the subject of an email notification
MAIL_LOADNAME="VEGA Transcript to Gene Model Sequence Association Load"

export MAIL_LOADNAME 
